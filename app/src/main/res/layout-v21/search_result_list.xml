<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools">

    <LinearLayout
        android:id="@+id/search_results"
        android:layout_margin="@dimen/margin_10"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="left"
                android:layout_weight="0.70"
                android:background="@drawable/admin_tbl_bg"
                android:padding="@dimen/margin_05"
                android:text="@string/mother_name_id"
                android:textColor="@color/black"
                android:textSize="@dimen/textsize_13sp"
                tools:ignore="RtlHardcoded" />

            <TextView
                android:id="@+id/mother_name_id"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="left"
                android:layout_weight="1"
                android:background="@drawable/admin_tbl_bg"
                android:padding="@dimen/margin_05"
                android:textColor="@color/black"
                android:textSize="@dimen/textsize_13sp"
                tools:ignore="RtlHardcoded" />

        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="left"
                android:layout_weight="0.70"
                android:background="@drawable/admin_tbl_bg"
                android:padding="@dimen/margin_05"
                android:text="@string/husband_name"
                android:textColor="@color/black"
                android:textSize="@dimen/textsize_13sp"
                tools:ignore="RtlHardcoded" />

            <TextView
                android:id="@+id/husband_father_name"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="left"
                android:layout_weight="1"
                android:background="@drawable/admin_tbl_bg"
                android:padding="@dimen/margin_05"
                android:textColor="@color/black"
                android:textSize="@dimen/textsize_13sp" />

        </LinearLayout>


        <LinearLayout
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="left"
                android:layout_weight="0.70"
                android:background="@drawable/admin_tbl_bg"
                android:padding="@dimen/margin_05"
                android:text="@string/mobile_no"
                android:textColor="@color/black"
                android:textSize="@dimen/textsize_13sp"
                tools:ignore="RtlHardcoded" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/mobile_no"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="left"
                android:layout_weight="1"
                android:background="@drawable/admin_tbl_bg"
                android:padding="@dimen/margin_05"
                android:textColor="@color/black"
                android:textSize="@dimen/textsize_13sp"
                tools:ignore="RtlHardcoded" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="left"
                android:layout_weight="0.70"
                android:background="@drawable/admin_tbl_bg"
                android:padding="@dimen/margin_05"
                android:text="@string/aadhar_no"
                android:textColor="@color/black"
                android:textSize="@dimen/textsize_13sp"
                tools:ignore="RtlHardcoded" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/aadhar_no"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="left"
                android:layout_weight="1"
                android:background="@drawable/admin_tbl_bg"
                android:padding="@dimen/margin_05"
                android:textColor="@color/black"
                android:textSize="@dimen/textsize_13sp"
                tools:ignore="RtlHardcoded" />
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="left"
                android:layout_weight="0.70"
                android:background="@drawable/admin_tbl_bg"
                android:padding="@dimen/margin_05"
                android:text="@string/janaadhar_no"
                android:textColor="@color/black"
                android:textSize="@dimen/textsize_13sp"
                tools:ignore="RtlHardcoded" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/janaadhar_no"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="left"
                android:layout_weight="1"
                android:background="@drawable/admin_tbl_bg"
                android:padding="@dimen/margin_05"
                android:textColor="@color/black"
                android:textSize="@dimen/textsize_13sp"
                tools:ignore="RtlHardcoded" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="left"
                android:layout_weight="0.70"
                android:background="@drawable/admin_tbl_bg"
                android:padding="@dimen/margin_05"
                android:text="@string/registration_date"
                android:textColor="@color/black"
                android:textSize="@dimen/textsize_13sp"
                tools:ignore="RtlHardcoded" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/registration_date"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="left"
                android:layout_weight="1"
                android:background="@drawable/admin_tbl_bg"
                android:padding="@dimen/margin_05"
                android:textColor="@color/black"
                android:textSize="@dimen/textsize_13sp"
                tools:ignore="RtlHardcoded" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="left"
                android:layout_weight="0.70"
                android:background="@drawable/admin_tbl_bg"
                android:padding="@dimen/margin_05"
                android:text="@string/lmp_date"
                android:textColor="@color/black"
                android:textSize="@dimen/textsize_13sp"
                tools:ignore="RtlHardcoded" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/lmp_date"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="left"
                android:layout_weight="1"
                android:background="@drawable/admin_tbl_bg"
                android:padding="@dimen/margin_05"
                android:textColor="@color/black"
                android:textSize="@dimen/textsize_13sp"
                tools:ignore="RtlHardcoded" />

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="left"
                android:layout_weight="0.70"
                android:background="@drawable/admin_tbl_bg"
                android:padding="@dimen/margin_05"
                android:text="@string/v_status"
                android:textColor="@color/black"
                android:textSize="@dimen/textsize_13sp"
                tools:ignore="RtlHardcoded" />

            <LinearLayout

                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/admin_tbl_bg"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/status"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@color/green"
                    android:gravity="center"
                    android:padding="@dimen/margin_05"
                    android:text="@string/pending_at_LS"
                    android:textColor="#fff"
                    android:textSize="@dimen/textsize_13sp" />

            </LinearLayout>
        </LinearLayout>


    </LinearLayout>

</RelativeLayout>